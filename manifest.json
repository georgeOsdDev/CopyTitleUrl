{
  "name": "Copy Page Title & URL",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "description": "An extension to copy page title & url.",
  "author": "Takeharu Oshida",
  "homepage_url": "https://github.com/georgeOsdDev/CopyTitleUrl",
  "icons": {
      "16": "icons/16x16.png",
      "32": "icons/32x32.png",
      "48": "icons/48x48.png",
      "128": "icons/128x128.png"
  },
  "action": {
      "default_popup": "popup/popup.html"
  },
  "permissions": ["tabs"]
}
